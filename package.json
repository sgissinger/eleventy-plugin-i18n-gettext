{
  "name": "eleventy-plugin-i18n-gettext",
  "version": "1.5.1",
  "description": "Eleventy plugin which adds i18n support with Gettext string translation and moment.js date and times localization",
  "main": ".eleventy.js",
  "types": "dist/i18n.d.ts",
  "scripts": {
    "test": "jest --silent",
    "prebuild": "rimraf dist",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgissinger/eleventy-plugin-i18n-gettext.git"
  },
  "keywords": [
    "eleventy",
    "eleventy-plugin",
    "11ty",
    "i18n",
    "gettext"
  ],
  "author": "Sebastien Gissinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sgissinger/eleventy-plugin-i18n-gettext/issues"
  },
  "homepage": "https://github.com/sgissinger/eleventy-plugin-i18n-gettext#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "node-gettext": "^3.0.0",
    "gettext-parser": "^4.0.4",
    "printf": "^0.5.3",
    "glob": "^7.1.6",
    "moment-timezone": "^0.5.31"
  },
  "devDependencies": {
    "typescript": "^3.9.7",
    "@types/node": "^14.0.27",
    "@types/glob": "^7.1.3",
    "jest": "^26.2.2",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 80,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
